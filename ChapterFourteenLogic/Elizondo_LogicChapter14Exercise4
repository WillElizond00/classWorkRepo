class Employee:
    def __init__(self, name, number):
        self.name = name
        self.number = number

#Accessor methods
    def get_name(self):
        return self.name
    
    def get_number(self):
        return self.number
    
#Mutator methods

    def set_name(self, name):
        self.name = name
    
    def set_number(self, number):
        self.number = number

